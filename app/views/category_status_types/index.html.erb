<p id="notice"><%= notice %></p>

<h1>Category Status Types</h1>

<table>
  <thead>
    <tr>
      <th>Status text</th>
      <th>Order in list</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @category_status_types.each do |category_status_type| %>
      <tr>
        <td><%= category_status_type.status_text %></td>
        <td><%= category_status_type.order_in_list %></td>
        <td><%= link_to 'Show', category_status_type %></td>
        <td><%= link_to 'Edit', edit_category_status_type_path(category_status_type) %></td>
        <td><%= link_to 'Destroy', category_status_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Category Status Type', new_category_status_type_path %>
